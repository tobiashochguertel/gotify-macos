# Environment variables for local act testing
# Copy this to .env.act.local and customize for your setup

# GitHub token for API access (optional for most workflows)
# GITHUB_TOKEN=your_github_token_here

# Test inputs for workflow_dispatch events
# INPUT_VERSION=v1.0.0-test
# INPUT_RELEASE_NOTES=Test release notes

# Secrets for testing (don't commit real secrets!)
# These are just for local testing
GOTIFY_HOST=localhost:8080
GOTIFY_TOKEN=test-token-for-local-testing

# Runner environment
RUNNER_TOOL_CACHE=/tmp/act-tool-cache
RUNNER_TEMP=/tmp/act-temp